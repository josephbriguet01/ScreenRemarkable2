<project name="ScreenRemarkable2" default="default" basedir=".">
    <description>Builds, tests, and runs the project ScreenRemarkable2.</description>
    <import file="nbproject/build-impl.xml"/>
    
    <target depends="init,compile,-pre-jar,-do-jar,-post-jar" description="Build JAR." name="jar">
        <property name="store.jar.name" value="ScreenRemarkable2"/>
        <property name="store.dir"      value="store"/>
        <property name="store.jar"      value="${store.dir}/${store.jar.name}.jar"/>
        <delete dir="${store.dir}"/>
        <mkdir  dir="${store.dir}"/>
        <jar destfile="${store.dir}/temp_final.jar" filesetmanifest="skip">
            <zipgroupfileset dir="dist"     includes="*.jar"/>
            <zipgroupfileset dir="dist/lib" includes="*.jar"/>
            <manifest>
                <attribute name="Main-Class" value="${main.class}"/>
                <attribute name="X-COMMENT" value="Main-Class will be added automatically by build"/>
                <attribute name="Build-Jdk-Spec" value="21"/>
                <attribute name="Specification-Title" value="JSch"/>
                <attribute name="Specification-Version" value="0.2"/>
                <attribute name="Implementation-Title" value="JSch"/>
                <attribute name="Implementation-Version" value="0.2.17"/>
                <attribute name="Multi-Release" value="true"/>
                <attribute name="Bundle-Description" value="JSch is a pure Java implementation of SSH2"/>
                <attribute name="Bundle-License" value="&quot;Revised BSD&quot;;link=&quot;https://github.com/mwiede/jsch/blob/master/LICENSE.txt&quot;,&quot;Revised BSD&quot;;link=&quot;https://github.com/mwiede/jsch/blob/master/LICENSE.JZlib.txt&quot;,ISC;link=&quot;https://github.com/mwiede/jsch/blob/master/LICENSE.jBCrypt.txt&quot;"/>
                <attribute name="Bundle-ManifestVersion" value="2"/>
                <attribute name="Bundle-Name" value="JSch"/>
                <attribute name="Bundle-SymbolicName" value="com.github.mwiede.jsch"/>
                <attribute name="Bundle-Version" value="0.2.17"/>
                <attribute name="Export-Package" value="com.jcraft.jsch;version=&quot;0.2.17&quot;"/>
                <attribute name="Import-Package" value="com.sun.jna;version=&quot;[5.0,6)&quot;;resolution:=optional,com.sun.jna.platform.win32;version=&quot;[5.0,6)&quot;;resolution:=optional,org.apache.logging.log4j;version=&quot;[2.0,4)&quot;;resolution:=optional,org.bouncycastle.crypto;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.digests;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.engines;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.generators;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.macs;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.modes;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.params;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.crypto.signers;version=&quot;[1.76,2)&quot;;resolution:=optional,org.bouncycastle.pqc.crypto.ntruprime;version=&quot;[1.76,2)&quot;;resolution:=optional,org.slf4j;version=&quot;[1.7,3)&quot;;resolution:=optional,org.newsclub.net.unix;resolution:=optional,org.ietf.jgss;resolution:=optional,javax.crypto,javax.crypto.interfaces,javax.crypto.spec"/>
                <attribute name="Require-Capability" value="osgi.ee;filter:=&quot;(&amp;(osgi.ee=JavaSE)(version=1.8))&quot;"/>
                <attribute name="Tool" value="Bnd-7.0.0.202310060912"/>
            </manifest>
        </jar>
        <zip destfile="${store.jar}">
            <zipfileset src="${store.dir}/temp_final.jar" excludes="META-INF/*.SF, META-INF/*.DSA, META-INF/*.RSA"/>
        </zip>
        <delete file="${store.dir}/temp_final.jar"/>
        <delete dir="dist"/>
        <delete dir="build"/>
        <delete dir="test"/>
        <rename src="${store.dir}" dest="dist"/>
    </target>
</project>